{
  "name": "react-examix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=21.6.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier src --ignore-path .gitignore --write --log-level warn",
