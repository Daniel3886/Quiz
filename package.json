{
  "name": "react_examix",
  "author": "Softjey",
  "version": "0.0.5",
  "scripts": {
    "dev": "concurrently \"cd client && npm run dev\"",
    "lint": "concurrently \"cd client && npm run lint\"",
    "install-all": "concurrently \"cd client && npm install\"",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "lint-staged": "^15.2.2"
  },
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": "npm run lint"
  }
}